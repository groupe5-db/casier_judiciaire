                <GridPane.margin>
                    <Insets bottom="5" left="0" right="10" top="0" />
                </GridPane.margin>
            </TextField>
            
            <Label text="NOM: " GridPane.columnIndex="2" GridPane.rowIndex="0">
                <GridPane.margin>
